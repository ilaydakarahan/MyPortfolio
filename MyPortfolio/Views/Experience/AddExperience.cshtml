@model MyPortfolio.Models.TblExperiences

@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<br />
<h3>Deneyim Ekleme Sayfası</h3>

<form method="post">
    <label>Başlangıç Yılı</label>
    <input type="text" name="StartYear" class="form-control"/>

    <label>Bitiş Yılı</label>
    <input type="text" name="EndYear" class="form-control"/>

    <label>Başlık</label>
    <input type="text" name="Title" class="form-control"/>

    <label>Açıklama</label>
    <input type="text" name="Description" class="form-control"/>

    <label>Firma</label>
    <input type="text" name="Company" class="form-control"/>

    <label>Konum</label>
    <input type="text" name="Location" class="form-control"/>
    <br />
    <button class="btn btn-outline-primary">Kaydet</button>

</form>

